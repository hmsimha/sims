$('#principal_override_request_sidebar').replaceWith("<%=escape_javascript( render_cell(:principal_overrides,:user_requests, :user=>current_user))%>");
$('#principal_override_response_sidebar').replaceWith("<%=escape_javascript( render_cell(:principal_overrides,:principal_responses, :user=>current_user))%>");
$('#<%=dom_id @principal_override,"acceptreject"%>').remove();
$('#<%=dom_id @principal_override,"pending_form_td"%>').remove();


$('#<%=dom_id @principal_override,"delete"%>').replaceWith("<%=escape_javascript(render @principal_override, :actions => [:delete])%>");
$('#tbody_undo').append("<%=escape_javascript(render @principal_override, :actions => [:undo])%>");
