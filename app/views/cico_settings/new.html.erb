<h1>New cico_setting</h1>

<% form_for(@cico_setting) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :school %><br />
    <%= f.text_field :school %>
  </p>
  <p>
    <%= f.label :probe_definition %><br />
    <%= f.text_field :probe_definition %>
  </p>
  <p>
    <%= f.label :enabled %><br />
    <%= f.check_box :enabled %>
  </p>
  <p>
    <%= f.label :default_participant %><br />
    <%= f.text_field :default_participant %>
  </p>
  <p>
    <%= f.label :points_per_expectation %><br />
    <%= f.text_field :points_per_expectation %>
  </p>
  <p>
    <%= f.label :default_goal %><br />
    <%= f.text_field :default_goal %>
  </p>
  <p>
    <%= f.label :days_to_collect %><br />
    <%= f.text_field :days_to_collect %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', cico_settings_path %>