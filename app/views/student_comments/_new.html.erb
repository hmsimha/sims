<%= div_for @student_comment do %>
  <%= form_for([@student,@student_comment], :format=>format, :html=>{:multipart => true, :target=>target}) do |f| %>
    <%= render :partial=>f %>
  <% end %>

  <%= link_to 'Cancel',@student, :class => "cancel_link", :data => {:remove => dom_id(@student_comment), :show => "new_team_note_link"} %>
<% end %>
