<% form_for([@student,@student_comment], :format => :js, :html=>{:multipart => true, :target=>"upload_frame", :method => :put}) do |f| %>
  <%= render :partial=>f %>
<% end %>
  

  <%= link_to_function 'cancel',{:href=> url_for(@student)} do |page|
    page[dom_id(@student_comment)].replace_html(:partial=>'student_comments/comment', :object=>@student_comment)
    end  %>
