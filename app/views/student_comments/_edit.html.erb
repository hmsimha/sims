<%=div_for @student_comment, "edit" do %>
<%= form_for([@student,@student_comment], :format => :js, :html=>{:multipart => true, :target=>"upload_frame", :method => :put}) do |f| %>
  <%= render :partial=>f %>
<% end %>

<%= link_to 'cancel', @student, :data => {:remove => dom_id(@student_comment, "edit"), :show => dom_id(@student_comment)}, :class => "cancel_link" %>
<% end %>
